#!/usr/bin/env node
'use strict';
const argv = require('minimist')(process.argv.slice(2));

if (argv.d === true || argv.debug === true) {
    console.log(argv);
}

if (argv.v === true) {
    const info = require('../package.json');
    console.log('v' + info.version);
    process.exit();
}

const say = require('../lib/say');
console.log(say.cheese.apply(say.cheese, argv._));
